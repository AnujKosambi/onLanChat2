<Window x:Class="SEN_project_v2.passworderror"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="passworderror" Height="100" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Text="Please try putting a password or disable the password enable field" ScrollViewer.VerticalScrollBarVisibility="Visible"
          />
        <Button Grid.Row="1" Content="Ok" VerticalAlignment="Center" HorizontalAlignment="Center" Width="50" Click="Button_Click"/>
    </Grid>
</Window>
